{
    "name": "marceauka/shaark",
    "type": "project",
    "description": "Your place to archive your links, stories, passwords and archive your content.",
    "keywords": [
        "laravel",
        "shaark",
        "shaarli",
        "links"
    ],
    "license": "MIT",
    "homepage": "https://github.com/MarceauKa/shaark/",
    "readme": "readme.md",
    "authors": [
        {
            "name": "Marceau Casals",
            "email": "marceau@casals.fr",
            "homepage": "https://marceau.casals.fr",
            "role": "Maintainer"
        }
    ],
    "support": {
        "email": "marceau@casals.fr",
        "issues": "https://github.com/MarceauKa/shaark/issues"
    },
    "require": {
        "php": "^8.0",
        "doctrine/dbal": "^3.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "hashids/hashids": "^4.1",
        "lab404/laravel-auth-checker": "^2.0",
        "laravel/framework": "^10.0",
        "laravel/scout": "^9.2",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^4.0",
        "maatwebsite/excel": "^3.1",
        "mews/captcha": "^3.2",
        "norkunas/youtube-dl-php": "^2.0",
        "predis/predis": "^1.1",
        "spatie/browsershot": "^3.57",
        "spatie/laravel-backup": "^8.0",
        "spatie/laravel-medialibrary": "^10.5",
        "spatie/valuestore": "^1.2",
        "symfony/dom-crawler": "^4.3",
        "teamtnt/laravel-scout-tntsearch-driver": "^13.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/dusk": "^7.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeds/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "provide": {
        "ext-imagick": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
